{
	"name": "promptab-extension",
	"version": "1.0.0",
	"description": "PrompTab - Intelligent browser extension for optimizing prompts",
	"private": true,
	"scripts": {
		"start": "webpack --watch --mode development",
		"build": "webpack --mode production",
		"build:analyze": "webpack --mode production --analyze",
		"build:fast": "webpack --mode production --no-source-map",
		"clean": "rm -rf dist",
		"clean:all": "rm -rf dist node_modules package-lock.json",
		"package": "npm run build && node scripts/package-crx.js",
		"package:crx": "node scripts/package-crx.js",
		"package:zip": "npm run build && node scripts/package-zip.js",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"test:fast": "jest --passWithNoTests",
		"lint": "eslint src --ext .ts,.tsx",
		"lint:fix": "eslint src --ext .ts,.tsx --fix",
		"lint:strict": "eslint src --ext .ts,.tsx --max-warnings 0",
		"type-check": "tsc --noEmit",
		"type-check:watch": "tsc --noEmit --watch",
		"format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
		"format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
		"prebuild": "npm run clean",
		"postbuild": "echo 'Build completed successfully!'",
		"dev": "npm run start",
		"prod": "npm run build"
	},
	"dependencies": {
		"@emotion/react": "^11.11.4",
		"@emotion/styled": "^11.11.5",
		"@mui/icons-material": "^5.16.0",
		"@mui/material": "^5.16.0",
		"@tanstack/react-query": "^5.51.0",
		"@types/node": "^24.0.12",
		"axios": "^1.7.2",
		"date-fns": "^4.1.0",
		"i18next": "^23.12.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-hot-toast": "^2.4.1",
		"react-i18next": "^15.0.0",
		"webextension-polyfill": "^0.12.0"
	},
	"devDependencies": {
		"@babel/core": "^7.24.7",
		"@babel/preset-env": "^7.24.7",
		"@babel/preset-react": "^7.24.7",
		"@babel/preset-typescript": "^7.24.7",
		"@testing-library/jest-dom": "^6.4.6",
		"@testing-library/react": "^16.0.0",
		"@testing-library/user-event": "^14.5.2",
		"@types/chrome": "^0.0.268",
		"@types/jest": "^29.5.12",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"@types/webextension-polyfill": "^0.10.7",
		"@typescript-eslint/eslint-plugin": "^7.15.0",
		"@typescript-eslint/parser": "^7.15.0",
		"archiver": "^7.0.1",
		"babel-loader": "^9.1.3",
		"copy-webpack-plugin": "^12.0.2",
		"crx3": "^1.1.3",
		"css-loader": "^7.1.2",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-react": "^7.34.3",
		"eslint-plugin-react-hooks": "^4.6.2",
		"html-webpack-plugin": "^5.6.0",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"prettier": "^3.3.2",
		"style-loader": "^4.0.0",
		"ts-jest": "^29.1.5",
		"ts-loader": "^9.5.1",
		"typescript": "^5.5.2",
		"webpack": "^5.92.1",
		"webpack-bundle-analyzer": "^4.10.1",
		"webpack-cli": "^5.1.4",
		"webpack-dev-server": "^5.0.4"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"engines": {
		"node": ">=18.0.0",
		"npm": ">=8.0.0"
	}
}